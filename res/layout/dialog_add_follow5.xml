
<ScrollView android:id="@+id/ScrollView01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical">
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginLeft="20px"
			android:layout_marginRight="20px" android:text="@string/layout_dialog_add_follow5_loginname"></TextView>
		<EditText android:layout_height="wrap_content"
			android:capitalize="none" android:id="@+id/add_follow5_username" android:layout_marginRight="20dip" android:layout_marginLeft="20dip" android:layout_width="150dip"></EditText>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginLeft="20px"
			android:layout_marginRight="20px" android:text="@string/layout_dialog_add_follow5__passwd"></TextView>
		<EditText android:layout_height="wrap_content"
			android:capitalize="none" android:id="@+id/add_follow5_passwd"
			android:password="true" android:layout_marginLeft="20dip" android:layout_marginRight="20dip" android:layout_width="150dip"></EditText>

	</LinearLayout>
</ScrollView>
